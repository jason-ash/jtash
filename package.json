    "@typescript-eslint/parser": "^5.9.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "gray-matter": "^4.0.3",
    "jstat": "^1.9.5",
    "pre-commit": "^1.2.2",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "rehype-highlight": "^5.0.2",
    "rehype-katex": "^6.0.2",
    "rehype-stringify": "^9.0.2",
    "remark-frontmatter": "^4.0.1",
